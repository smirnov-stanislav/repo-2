class Road:
    __length = None
    __width = None
    weigth = None
    tickness = None
    def __init__(self, length, width):
        self.length = length
        self.width = width
        print('Создаем проект')

    def intake(self):
        self.weigth = 20
        self.tickness = 0.1
        intake = self.length * self.width * self.weigth * self.tickness / 1000
        print(f'нужно {intake} тонн')

road_to_village = Road(5000, 1)
road_to_village.intake()